#include<stdio.h>

int main(){
	int a,b,c;
	printf("Input:");
	scanf("%d%d%d",&a,&b,&c);

	if(a<c && b<c)
	{	
		printf("the result is: %d",a+b);
	}
	if(b<a && c<a)
	{
		printf("the result is: %d",c+b);
	}
	if(a<b && c<b)
	{
		printf("the result is: %d",a+c);
	}
	if(a==b==c) 
	{
		printf("All three numbers are equal.");
	}
return 0;
}
