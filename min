#include <stdio.h>
#include <conio.h>
main()
{
	int x,y,z;	
	printf("Enter number : ");
	scanf("%d", &x);
	scanf("%d", &y);
	scanf("%d", &z);

	if(x<y&&x<z)
	{
		printf("Minimum = %d",x);
	}
	if(y<x&&y<z)
	{
		printf("Minimum = %d",y);
	}
	if(z<y&&z<x)
	{
		printf("Minimum = %d",z);
	}
    getch();
}
